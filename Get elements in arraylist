//get elements in arraylist

import java.util.*;

public class ArrayLists1 {
    public static void main(String[]args){
        ArrayList<Integer>list=new ArrayList<Integer>();

        list.add(1);
        list.add(2);
        list.add(3);
        list.add(1);

        int ele=list.get(0);
        System.out.println(ele);

    }
}
