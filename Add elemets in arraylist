//add element in between 
import java.util.ArrayList;

public class ArrayLists2 {
    public static void main(String[]args){
        ArrayList<Integer>list=new ArrayList<Integer>();

        list.add(1);
        list.add(2);
        list.add(3);
        list.add(1);

        list.add(1,1);
        System.out.println(list);
    }
}
