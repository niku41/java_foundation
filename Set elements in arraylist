//set element in arraylist

import java.util.ArrayList;

public class arrayLists3 {
    public static void main(String[]args){
        ArrayList<Integer>list=new ArrayList<Integer>();

        list.add(1);
        list.add(2);
        list.add(3);
        list.add(1);

        list.set(0,5);
        System.out.println(list);
    }

}
