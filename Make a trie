//Make a trie

public class TrieYt {
    static class Node{
        Node[]children;
        boolean endOfWord;

        public Node(){
            children=new Node[26];
            for(int i=0;i<26;i++){
                children[i]=null;
            }
            endOfWord=false;
        }
    }

    static Node root=new Node();  

    public static void main(String[]args){
        String words[]={"the","a","there","their","any"};
    }

}
