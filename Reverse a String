import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner stringScanner = new Scanner(System.in);
        String str = stringScanner.next();
        int length=str.length();
        String rev="";
        for(int i=length-1;i>=0;i--){
            rev=rev+str.charAt(i);
        }
       System.out.println(rev);
	}
}
