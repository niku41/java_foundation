import java.util.Scanner;
public class Number1 {
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int even_count=0;
        int odd_count=0;
        while(num>0) {
            int rem = num % 10;
            if (rem % 2 == 0) {
                even_count++;
            } else {
                odd_count++;
            }
            num=num/10;
            System.out.println("even numbers 143"+even_count);
            System.out.println("odd numbers "+odd_count);
        }
    }
}
