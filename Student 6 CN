public class Student6 {
    public String name;
    final private int rollNumber;
    double cgpa;
    public final static double CONVERSIONFACTOR=0.95;
    private static int numStudents;
    public Student6 (String name){
        this.name=name;
        numStudents++;
        this.rollNumber=numStudents;
    }
    public Student6(){
        name="abc";
        numStudents++;
        this.rollNumber=numStudents;
    }
    public int getNumStudent6(){
        return numStudents;
    }
    public void print(){
        System.out.println(name+" "+rollNumber);
    }
}






import java.util.Scanner;

public class Student6Use {
    public static void main(String[]args) {
       // System.out.print(Student6.getNumStudent6());
        Student6 S1=new Student6("nikita");
    }
}
