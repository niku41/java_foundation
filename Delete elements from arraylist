//delete elemet from arraylist

import java.util.ArrayList;

public class ArrayLists4 {
    public static void main(String[]args){
        ArrayList<Integer>list=new ArrayList<Integer>();

        list.add(1);
        list.add(2);
        list.add(3);
        list.add(1);

        list.remove(3);
        System.out.println(list);
    }
}
