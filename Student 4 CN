//Final keyword
public class Student4 {
    public String name;
    final private int rollNumber;
    double cgpa;
    final double CONVERSIONFACTOR=0.95;
    public Student4(String name,int rollNumber){
        this.name=name;
        this.rollNumber=rollNumber;
    }
}



import java.util.Scanner;

//import static jdk.internal.reflect.AccessorGenerator.S5;

public class Student4Use {
    public static void main(String[]args) {
        Scanner sc = new Scanner(System.in);
        Student4 S1 = new Student4("nikita", 123);
        System.out.println(S1);
       // S1.CONVERSIONFACTOR =9;
    }
}
